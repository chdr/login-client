{
  "extends": [
    "react-app",
    "airbnb"
  ],
  "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "comma-dangle": ["error", "never"],
      "jsx-a11y/label-has-for": [ 2, {
        "components": [ "Label" ],
        "required": {
          "every": [ "id" ]
        },
        "allowChildren": false
      }],
      "jsx-a11y/label-has-associated-control": false
    }
}
